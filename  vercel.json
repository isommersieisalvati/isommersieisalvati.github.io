{
  "builds": [
    {
      "src": "hugo.toml",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "buildCommand": "hugo --baseURL=https://heatingale/blog/",
  "env": {
    "HUGO_VERSION": "0.147.3"
  },
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/$1"
    }
  ]
}
